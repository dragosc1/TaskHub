@{
}
<h1>Echipele din care faci parte: </h1>
<br />

<div class="card-deck">
    @foreach (var echipa in ViewBag.Echipe)
    {
        Echipa e = echipa as Echipa;
        <div class="card">
            <div class="card-body">
                <p>Faci parte din echipa cu proiectul @e.Proiect.NumeProiect</p>
            </div>
        </div>
    }
</div>