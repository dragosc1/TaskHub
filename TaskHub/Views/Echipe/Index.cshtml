@{
    ViewBag.Title = "Index";
}
<h1>Echipele din care faci parte: </h1>
<br />

<div class="card-deck">
    @foreach (var echipa in ViewBag.Echipe)
    {
        Echipa e = echipa as Echipa;
        <div class="card" style="background-color: #CDCFD0">
            <div class="card-body">
                <h4>    Echipa: @e.Proiect.NumeProiect</h4>
                <button style="padding-left: 1vw; padding-right: 1vw; border-radius: 5%; background-color:#697184"><a href="/Echipe/Show/@echipa.IdProiect" class="text-white" style="text-decoration:none; font-size: 2vh">Afisare echipa</a></button>
            </div>
        </div>
        <br />
        <br />
    }
</div>